{
  "name": "flow_pilot",
  "version": "0.0.1",
  "private": true,
  "description": "FlowPilot 最好的全链路求职服务AI平台",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:core-ai": "turbo run build --filter=@jobPilot/core-ai",
    "build:core-db": "turbo run build --filter=@jobPilot/core-db",
    "build:core-vectors": "turbo run build --filter=@jobPilot/core-vectors",
    "build:core-agents": "turbo run build --filter=@jobPilot/core-agents",
    "build:core-prompts": "turbo run build --filter=@jobPilot/core-prompts",
    "build:core-ui": "turbo run build --filter=@jobPilot/core-ui",
    "build:core-lib": "turbo run build --filter=@jobPilot/core-lib"
  },
  "keywords": [
    "flow_pilot",
    "flow_pilot_ai",
    "flow_pilot_ai_platform",
    "flow_pilot_ai_platform_for_recruitment"
  ],
  "author": "FlowPilot",
  "license": "MIT",
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.0.1",
    "dotenv": "16.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-vue": "^10.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsc": "2.0.4",
    "tsup": "^8.5.0",
    "turbo": "^2.5.4",
    "typescript": "^5.3.3"
  }
}
