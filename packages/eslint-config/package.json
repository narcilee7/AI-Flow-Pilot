{
  "name": "@jobPilot/eslint-config",
  "version": "0.0.3",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "import": "./dist/react-internal.mjs",
      "require": "./dist/react-internal.cjs"
    },
    "./vue": {
      "import": "./dist/vue-internal.mjs",
      "require": "./dist/vue-internal.cjs"
    },
    "./node": {
      "import": "./dist/node-internal.mjs",
      "require": "./dist/node-internal.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "rollup": "^4.16.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-vue": "^10.2.0"
  }
}
